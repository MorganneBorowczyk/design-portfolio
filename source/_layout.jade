    mixin btn-large(text, link)
      a.btn.btn-large(href=link)= text
    mixin btn-small(text, link)
      a.btn.btn-small(href=link)= text
      
    mixin grid-item(name, id, size, target, source, source2, source3)
      a.grid-item(class=size href="#"+target)
        img(src="assets/images/work/"+source alt=name)  
      .modal(id=target)
          .modal-content
            h2=name
            .close Close
            img(src="assets/images/work/"+source)
            - if (source2)
              img(src="assets/images/work/"+source2)
            - if (source3)
              img(src="assets/images/work/"+source3)
    
    
    
